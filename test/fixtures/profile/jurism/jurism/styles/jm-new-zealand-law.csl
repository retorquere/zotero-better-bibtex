<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" default-locale="en" version="1.1mlz1" and="text" delimiter-precedes-last="never" initialize="false" initialize-with="" sort-separator=", ">
  <info>
    <title>JM New Zealand Law Style</title>
    <id>http://juris-m.github.io/styles/jm-new-zealand-law</id>
    <link href="http://juris-m.github.io/styles/jm-new-zealand-law" rel="self"/>
    <link href="http://www.lawfoundation.org.nz/style-guide/index.html" rel="documentation"/>
    <author>
      <name>Julia Caldwell</name>
      <email>caldwelljules@gmail.com</email>
    </author>
    <contributor>
      <name>Jack Newberry</name>
    </contributor>
    <contributor>
      <name> John Prebble </name>
      <email> john.prebble@vuw.ac.nz</email>
    </contributor>
    <contributor>
      <name> Frank Bennett </name>
    </contributor>
    <category citation-format="note"/>
    <category field="law"/>
    <updated>2020-11-02T14:05:40+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <style-options punctuation-in-quote="false"/>
    <date form="text">
      <date-part form="numeric" name="day" suffix=" "/>
      <date-part name="month" suffix=" "/>
      <date-part name="year"/>
    </date>
    <date form="numeric">
      <date-part form="numeric-leading-zeros" name="day" suffix="/"/>
      <date-part form="numeric-leading-zeros" name="month" suffix="/"/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="and" form="symbol">&amp;</term>
      <term name="article">article</term>
      <term form="short" name="article">art.</term>
      <term name="director">director</term>
      <term form="verb-short" name="director">dir by</term>
      <term form="short" name="director">dir</term>
      <term form="verb" name="director">directed by</term>
      <term form="long" name="Section">
        <single>section</single>
        <multiple>sections</multiple>
      </term>
      <term form="short" name="Section">
        <single>s</single>
        <multiple>ss</multiple>
      </term>
      <term form="long" name="Chapter">
        <single>chapter</single>
        <multiple>chapters</multiple>
      </term>
      <term form="short" name="Chapter">
        <single>ch.</single>
        <multiple>chs.</multiple>
      </term>
      <term form="symbol" name="Chapter">
        <single>c.</single>
        <multiple>cc.</multiple>
      </term>
      <term name="title">title</term>
      <term form="short" name="title">tit.</term>
      <term form="short" name="chapter">
        <single>ch</single>
        <multiple>chs</multiple>
      </term>
      <term form="short" name="column">
        <single>col</single>
        <multiple>cols</multiple>
      </term>
      <term form="short" name="figure">
        <single>fig</single>
        <multiple>figs</multiple>
      </term>
      <term form="short" name="issue">
        <single>no</single>
        <multiple>nos</multiple>
      </term>
      <term form="short" name="line">
        <single>l</single>
        <multiple>ll</multiple>
      </term>
      <term form="short" name="note">
        <single>n</single>
        <multiple>nn</multiple>
      </term>
      <term form="short" name="opus">op</term>
      <term form="short" name="page">
        <single>p</single>
        <multiple>pp</multiple>
      </term>
      <term form="short" name="paragraph">
        <single>para</single>
        <multiple>paras</multiple>
      </term>
      <term name="paragraph" form="static">[%s]</term>
      <term form="short" name="part">
        <single>pt</single>
        <multiple>pts</multiple>
      </term>
      <term form="short" name="section">
        <single>s</single>
        <multiple>ss</multiple>
      </term>
      <term form="short" name="sub verbo">sv</term>
      <term form="short" name="verse">vrs</term>
      <term form="short" name="volume">
        <single>vol</single>
        <multiple>vols</multiple>
      </term>
      <term form="short" name="edition">
        <single>ed</single>
        <multiple>eds</multiple>
      </term>
      <term form="short" name="supplement">
        <single>supp</single>
        <multiple>supps</multiple>
      </term>
      <term name="et-al">and others</term>
      <term name="ordinal-01">st</term>
      <term name="ordinal-02">nd</term>
      <term name="ordinal-03">rd</term>
      <term name="ordinal-04">th</term>
      <term form="short" name="editor">
        <single>(ed)</single>
        <multiple>(eds)</multiple>
      </term>
      <term name="open-quote">&#8220;</term>
      <term name="close-quote">&#8221;</term>
      <term name="open-inner-quote">&#8216;</term>
      <term name="close-inner-quote">&#8217;</term>
    </terms>
  </locale>
  <macro name="title-short">
    <choose>
      <if type="legal_case">
        <text font-style="italic" form="short" variable="title-short"/>
      </if>
      <else-if type="book">
        <text font-style="italic" form="short" variable="title"/>
      </else-if>
      <else-if type="entry-encyclopedia entry-dictionary legislation bill">
        <text variable="title"/>
      </else-if>
      <else-if match="all" type="motion_picture">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else-if variable="container-title">
        <text quotes="true" variable="title"/>
      </else-if>
      <else-if match="all" type="broadcast">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text quotes="true" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <choose>
      <if match="none" type="broadcast motion_picture">
        <names variable="editor">
          <name/>
          <et-al term="et-al"/>
          <label form="short" prefix=" "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="editor-bib">
    <names variable="editor">
      <name/>
      <et-al term="et-al"/>
      <label form="short" prefix=" "/>
    </names>  	
  </macro>
  <macro name="translator">
    <names variable="translator">
      <name/>
      <label form="long" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="translator-bib">
    <names variable="translator">
      <name/>
      <et-al term="et-al"/>
      <label form="long" prefix=" (" suffix=")"/>
    </names>		
  </macro>
  <macro name="container-title">
    <text form="short" strip-periods="true" variable="container-title"/>
  </macro>
  <macro name="author-note">
    <choose>
      <if type="broadcast" match="none">
        <names variable="author">
          <name/>
          <et-al term="et-al"/>
          <institution delimiter=", " use-first="1" use-last="1"/>
          <substitute>
            <names variable="composer"/>
            <names variable="container-author"/>
            <names variable="editor"/>
            <choose>
              <if type="video">
                <names variable="contributor"/>
              </if>
            </choose>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author-bib">
    <names variable="author">
      <name/>
      <et-al term="et-al"/>
      <institution delimiter=", " use-first="1" use-last="1"/>
      <substitute>
        <names variable="composer"/>
        <names variable="container-author"/>
        <text macro="editor-bib"/>
        <text macro="translator-bib"/>
        <choose>
          <if type="video">
            <names variable="contributor"/>
          </if>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="issued-long">
    <date variable="issued">
      <date-part name="day" suffix=" "/>
      <date-part form="long" name="month" suffix=" "/>
      <date-part form="long" name="year"/>
    </date>
  </macro>
  <macro name="edition">
    <group delimiter=" ">
      <number form="ordinal" text-case="lowercase" variable="edition"/>
      <text form="short" strip-periods="true" term="edition"/>
    </group>
  </macro>
  <macro name="volume-number">
    <group delimiter=" ">
      <choose>
        <if is-numeric="volume">
          <label form="short" variable="volume"/>
        </if>
      </choose>
      <number variable="volume"/>
    </group>
  </macro>
  <macro name="sort-by-type">
    <choose>
      <if match="any" type="legal_case">
        <text value="1"/>
      </if>
      <else-if match="any" type="bill legislation">
        <text value="2"/>
      </else-if>
      <else-if match="any" type="book chapter">
        <text value="3"/>
      </else-if>
      <else-if match="any" type="article-journal">
        <text value="4"/>
      </else-if>
      <else-if type="report">
        <choose> 
          <if jurisdiction="gb">
            <text value="5"/>
          </if>
          <else-if jurisdiction="nz">
            <text value="5"/>
          </else-if>
          <else>
            <text value="6"/>
          </else>
        </choose>
      </else-if>
      <else-if type="thesis">
        <text value="7"/>
      </else-if>
      <else-if match="any" type="webpage post post-weblog">
        <text value="8"/>
      </else-if>
      <else>
        <text value="9 "/>
      </else>
    </choose>
  </macro>
  <macro name="render-chapter">
    <group delimiter=" ">
      <text macro="title-short"/>
      <choose>
        <if type="broadcast">
          <group delimiter=" ">
            <text term="director" form="verb"/>
            <names variable="author">
              <name/>
              <et-al term="et-al"/>
            </names>
          </group>
        </if>
      </choose>
      <choose>
        <if match="any" type="broadcast">
          <text value="on"/>
        </if>
        <else>
          <text form="short" term="in"/>
        </else>
      </choose>
      <text macro="editor" strip-periods="true" suffix=""/>
      <text font-style="italic" macro="container-title"/>
      <text macro="publisher-place-year"/>
      <number variable="page-first"/>
    </group>
  </macro>
  <macro name="render-article-journal">
    <choose>
      <if jurisdiction="nz">
        <text macro="render-hansard"/>
      </if>
      <else>
        <group delimiter=" ">
          <text macro="title-short"/>
          <choose>
            <if variable="volume issue" match="any">
              <date date-parts="year" form="text" prefix="(" suffix=")" variable="issued"/>
            </if>
            <else>
              <date date-parts="year" form="text" prefix="[" suffix="]" variable="issued"/>
            </else>
          </choose>
          <choose>
            <if variable="volume">
              <group>
                <number variable="volume"/>
                <choose>
                  <if type="article-magazine">
                    <number prefix="(" suffix=")" variable="issue"/>
                  </if>
                </choose>
              </group>
              </if>
              <else>
                <number variable="issue"/>
              </else>
          </choose>
          <text macro="container-title"/>
          <number variable="page-first"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="render-book">
    <group delimiter=" ">
      <group delimiter=", ">
        <text macro="title-short"/>
        <choose>
          <if variable="author">
            <text macro="editor"/>
          </if>
        </choose>
      </group>
      <text macro="publisher-place-year"/>
      <text macro="volume-number"/>
    </group>
  </macro>
  <macro name="render-thesis">
    <group delimiter=" ">
      <text macro="title-short"/>
      <group delimiter=", " prefix="(" suffix=")">
        <text variable="genre"/>
        <text form="short" variable="publisher"/>
        <date date-parts="year" form="text" variable="issued"/>
      </group>
    </group>
  </macro>
  <macro name="render-paper-conference">
    <group delimiter=" ">
      <text macro="title-short"/>
      <group delimiter=" ">
        <text term="in"/>
        <text font-style="italic" variable="container-title"/>
      </group>
      <group delimiter=", " prefix="(" suffix=")">
        <text form="short" variable="publisher"/>
        <choose>
          <if variable="event">
            <group delimiter=" ">
              <choose>
                <if variable="genre">
                  <text form="short" variable="genre"/> 
                </if>
                <else>
                  <text value="paper presented to"/> 
                </else>
              </choose>
              <text variable="event"/>
            </group>
            <text variable="publisher-place"/>
          </if>
        </choose>
        <text macro="issued-long"/>
      </group>
      <number variable="page-first"/>
    </group>
  </macro>
  <macro name="render-article-newspaper">
    <group delimiter=" ">
      <text macro="title-short"/>
      <text font-style="italic" macro="container-title"/>
      <group delimiter=", " prefix="(" suffix=")">
        <text variable="genre"/>
        <text macro="multi-media-medium"/>
        <text variable="publisher-place"/>
        <text macro="issued-long"/>
      </group>
      <choose>
        <if type="video broadcast">
          <text macro="volume-number"/>
        </if>
      </choose>
      <number variable="page-first"/>
    </group>
  </macro>
  <macro name="render-speech">
    <group delimiter=" ">
      <text macro="title-short"/>
      <group delimiter=", " prefix="(" suffix=")">
        <text variable="event"/>
        <text variable="publisher-place"/>
        <text macro="issued-long"/>
      </group>
    </group>
  </macro>
  <macro name="render-interview">
    <group delimiter=" ">
      <text macro="author-note" prefix="Interview with "/>
      <group delimiter=", " prefix="(" suffix=")">
        <names variable="interviewer">
          <name/>
          <et-al term="et-al"/>
        </names>
        <text variable="medium"/>
        <text macro="issued-long"/>
      </group>
      <group>
        <text value="transcript provided by "/>
        <names variable="translator">
          <name/>
        </names>
      </group>
    </group>
  </macro>
  <macro name="render-email">
    <group delimiter=" ">
      <choose>
        <if variable="genre">
          <group delimiter=" ">
            <text text-case="capitalize-first" variable="genre"/>
            <text value="from"/>
          </group>
        </if>
        <else>
          <text value="Email from"/>
        </else>
      </choose>
      <names variable="author">
        <name/>
        <institution delimiter=" ">
          <institution-part if-short="true" name="long" prefix="(" suffix=")"/>
        </institution> 
      </names>	 
      <names variable="recipient">
        <name prefix="to "/>
        <institution delimiter=" ">
          <institution-part if-short="true" name="long" prefix="(" suffix=")"/>
        </institution> 
      </names>
      <text macro="title-short"/>
      <text macro="issued-long" prefix="(" suffix=")"/>
    </group>
  </macro>
  <macro name="render-podcast">
    <group delimiter=" ">
      <text macro="title-short"/>
      <group delimiter=" ">
        <text value="on"/>
        <text font-style="italic" macro="container-title"/>
      </group>
      <group delimiter=", " prefix="(" suffix=")">
        <choose>
          <if variable="collection-title">
            <text macro="finish-podcast-as-podcast"/>
          </if>
          <else-if match="all" variable="container-title medium">
            <text macro="finish-podcast-as-song"/>
          </else-if>
          <else-if variable="composer">
            <text macro="finish-podcast-as-song"/>
          </else-if>
          <else-if has-year-only="issued">
            <text macro="finish-podcast-as-song"/>
          </else-if>
          <else>
            <text macro="finish-podcast-as-podcast"/>
          </else>
        </choose>
      </group>
      <text variable="collection-title"/>
    </group>
  </macro>
  <macro name="finish-podcast-as-song">
    <group delimiter=", ">
      <text variable="medium"/>
      <text form="short" variable="publisher"/>
      <date date-parts="year" form="text" variable="issued"/>
    </group>
  </macro>
  <macro name="finish-podcast-as-podcast">
    <group delimiter=", ">
      <text value="Podcast"/>
      <text macro="issued-long"/>
    </group>
  </macro>
  <macro name="render-blogpost">
    <group delimiter=" ">
      <text macro="title-short"/>
      <choose>
        <if type="post">
          <text form="short" variable="genre"/>
        </if>
      </choose>
      <text macro="issued-long" prefix="(" suffix=")"/>
      <text variable="container-title"/>
    </group>
  </macro>
  <macro name="render-encyclopedia">
    <choose>
      <if jurisdiction="nz">
        <group delimiter=" ">
          <text font-style="italic" variable="container-title"/>
          <text macro="title-short"/>
          <choose>
            <if variable="URL">
              <text value="(online ed)"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <group delimiter=" ">
          <text font-style="italic" variable="container-title"/>
          <group delimiter=", " prefix="(" suffix=")">
            <text macro="edition"/>
            <choose>
              <if match="none" variable="genre URL">
                <text variable="publisher"/>
              </if>
            </choose>
            <text variable="collection-title"/>
            <date date-parts="year" form="text" variable="issued"/>
            <choose>
              <if variable="genre">
                <text variable="genre"/>
              </if>
            </choose>
          </group>
          <text macro="volume-number"/>
          <text macro="title-short"/>
        </group>
      </else>
    </choose>
  </macro>

  <macro name="std-locator-label">
    <group delimiter=" ">
      <label form="short" variable="locator"/>
      <number variable="locator"/>
    </group>
  </macro>
  <macro name="std-authority-child-shy">
    <names variable="authority">
      <name/>
      <institution institution-parts="short" stop-first="1" use-last="1"/>
    </names>
  </macro>
  <macro name="std-authority-key">
    <names variable="authority">
      <name/>
      <institution form="short"/>
    </names>
  </macro>
  <macro name="std-authority-parent">
    <names variable="authority">
      <name/>
      <institution institution-parts="short" use-first="1"/>
    </names>
  </macro>
  <macro name="std-inventor-label">
    <choose>
      <if is-plural="author">
        <text value="inventors"/>
      </if>
      <else>
        <text value="inventor"/>
      </else>
    </choose>
  </macro>
  <macro name="std-patent-detail">
    <group delimiter=", ">
      <text variable="title"/>
      <group delimiter=" ">
        <text form="short" variable="jurisdiction"/>
        <choose>
          <if variable="genre">
            <text variable="genre"/>
            <text value="no."/>
          </if>
          <else>
            <text value="Patent No."/>
          </else>
        </choose>
        <number variable="number"/>
      </group>
    </group>
  </macro>
  <macro name="std-assignee-label">
    <choose>
      <if is-plural="recipient">
        <text value="assignees"/>
      </if>
      <else>
        <text value="assignee"/>
      </else>
    </choose>
  </macro>
  <macro name="std-locator-paragraph-bracket">
    <group delimiter=" ">
      <choose>
        <if locator="page paragraph" match="none">
          <label form="short" variable="locator"/>
        </if>
      </choose>
      <choose>
        <if locator="paragraph">
          <number prefix="[" suffix="]" variable="locator"/>
        </if>
        <else>
          <number variable="locator"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="render-hansard">
    <group delimiter=" ">
      <text macro="issued-long" prefix="(" suffix=")"/>
      <number variable="volume"/>
      <text macro="container-title"/>
      <number variable="page-first"/>
      <text variable="collection-title"/>
    </group>
  </macro>
  <macro name="render-report">
    <choose>
      <if jurisdiction="nz;maori">
        <group delimiter=" ">
          <text font-style="italic" strip-periods="true" variable="title"/>
          <group delimiter=", " prefix="(" suffix=")">
            <number variable="number"/>
            <date date-parts="year" form="text" variable="issued"/>
          </group>
          <text macro="volume-number"/>
        </group>
      </if>
      <else-if jurisdiction="nz">
        <group delimiter=" ">
          <choose>
            <if variable="genre">
              <text font-style="italic" strip-periods="true" variable="title"/>
              <group delimiter=" " prefix="(" suffix=")">
                <text variable="genre"/>
                <group delimiter=", ">
                  <number variable="number"/>
                  <date date-parts="year" form="text" variable="issued"/>
                </group>
              </group>
            </if>
            <else-if match="all" variable="collection-title">
              <group delimiter=" ">
                <text quotes="true" variable="title"/>
                <number variable="number"/>
                <group prefix="(" suffix=")">
                  <text macro="issued-long"/>
                </group>
                <text font-style="italic" variable="collection-title"/>
              </group>
            </else-if>
            <else>
              <text quotes="true" variable="title"/>
              <text macro="issued-long" prefix="(" suffix=")"/>
              <number variable="number"/>
            </else>
          </choose>
        </group>
      </else-if>
      <else>
        <group delimiter=" ">
          <text font-style="italic" strip-periods="true" variable="title"/>
          <group delimiter=" " prefix="(" suffix=")">
            <number variable="number"/>
            <text form="short" variable="publisher"/>
            <date date-parts="year" form="text" variable="issued"/>
          </group>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="hereinafter">
    <group prefix="[" suffix="]">
      <choose>
        <if type="legal_case book report">
          <text font-style="italic" variable="hereinafter"/>
        </if>
        <else>
          <text quotes="true" variable="hereinafter"/>
        </else>
      </choose>
    </group>
  </macro>
  
  <macro name="pinpoint">
    <choose>
      <if jurisdiction="us" match="all" position="first" type="legal_case"/>
      <else>
        <group delimiter=" ">
          <choose>
            <if locator="paragraph" match="all" variable="locator">
              <text term="at"/>
              <number variable="locator-extra"/>
              <choose>
                <if locator="paragraph" type="patent">
                  <number prefix="[" suffix="]" variable="locator"/>
                </if>
                <else-if jurisdiction="us">
                  <label form="short" variable="locator"/>
                  <number label-form="short" variable="locator"/>
                </else-if>
                <else>
                  <number prefix="[" suffix="]" variable="locator"/>
                </else>
              </choose>
            </if>
            <else-if locator="page">
              <text term="at"/>
              <number label-form="short" variable="locator"/>
            </else-if>
            <else-if locator="section">
              <choose>
                <if is-numeric="locator">
                  <text term="at"/>
                  <label form="short" variable="locator"/>
                  <number label-form="short" variable="locator"/>
                </if>
                <else>
                  <number variable="locator"/>
                </else>
              </choose>
            </else-if>
            <else>
              <text term="at"/>
              <label form="short" variable="locator"/>
              <number label-form="short" variable="locator"/>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  
  <macro name="subsequent-bill">
    <group delimiter=" ">
      <text variable="title"/>
      <choose>
        <if disambiguate="true">
          <date date-parts="year" form="text" variable="issued"/>
        </if>
      </choose>
    </group>
  </macro>
  
  <macro name="short-form">
    <choose>
      <if match="any" type="bill legislation">
        <text macro="subsequent-bill"/>
      </if>
      <else-if type="legal_case">
        <choose>
          <if variable="hereinafter">
            <text font-style="italic" variable="hereinafter"/>
          </if>
          <else-if variable="title-short">
            <text font-style="italic" form="short" strip-periods="true" variable="title-short"/>
          </else-if>
          <else>
            <text font-style="italic" form="short" strip-periods="true" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else>
        <choose>
          <if match="any" variable="author composer editor container-author">
            <choose>
              <if type="broadcast">
                <text macro="title-short"/>
              </if>
              <else>
                <names variable="author">
                  <name form="short"/>
                  <substitute>
                    <names variable="composer"/>
                    <names variable="container-author"/>
                    <names variable="editor"/>
                  </substitute>
                </names>
              </else>
            </choose>
          </if>
          <else>
            <choose>
              <if variable="hereinafter">
                <text font-style="italic" variable="hereinafter"/>
              </if>
              <else>
                <text macro="title-short"/>
              </else>
            </choose>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="multi-media-medium">
    <choose>
      <if match="any" type="broadcast motion_picture video">
        <text form="short" variable="medium"/>
      </if>
    </choose>
  </macro>
  <macro name="publisher-place-year">
    <group delimiter=", " prefix="(" suffix=")">
      <text variable="genre"/>
      <names variable="director">
        <name/>
        <label form="short" prefix=" (" suffix=")"/>
      </names>
      <text macro="translator"/>
      <text macro="multi-media-medium"/>
      <text macro="edition"/>
      <group delimiter=", ">
        <choose>
          <if variable="publisher">
            <text form="short" variable="publisher"/>
            <text variable="publisher-place"/>
          </if>
        </choose>
      </group>
      <choose>
        <if type="broadcast">
          <date date-parts="year-month-day" form="text" variable="issued"/>
        </if>
        <else>
          <date date-parts="year" form="text" variable="issued"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="access-url">
    <choose>
      <if match="any" type="article-newspaper graphic post-weblog post song webpage motion_picture broadcast video map">
        <text prefix="&lt;" suffix="&gt;" variable="URL" form="short"/>    
      </if>
      <else-if match="any" type="entry-dictionary entry-encyclopedia">
        <choose>
          <if match="none" variable="genre">
            <text prefix="&lt;" suffix="&gt;" variable="URL"/>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="std-locator-non-page-label-at">
    <group delimiter=" ">
      <choose>
        <if locator="page">
          <text term="at"/>
        </if>
        <else>
          <label form="short" variable="locator"/>
        </else>
      </choose>
      <number variable="locator"/>
    </group>
  </macro>
  <macro name="std-locator-non-page-label">
    <group delimiter=" ">
      <choose>
        <if locator="page" match="none">
          <label form="short" variable="locator"/>
        </if>
      </choose>
      <number variable="locator"/>
    </group>
  </macro>
  <macro name="jurisdictions-patent">
    <group delimiter=" ">
      <text macro="std-patent-first"/>
    </group>
  </macro>
  <macro name="std-patent-first">
    <group delimiter=", ">
      <group delimiter="; ">
        <group delimiter=" ">
          <names variable="author">
            <name/>
          </names>
          <text macro="std-inventor-label" prefix="(" suffix=")"/>
        </group>
        <group delimiter=" ">
          <names variable="recipient">
            <name/>
          </names>
          <text macro="std-assignee-label" prefix="(" suffix=")"/>
        </group>
      </group>
      <group delimiter=", ">
        <text macro="std-patent-detail"/>
        <group delimiter=", " prefix="(" suffix=")">
          <group delimiter=" ">
            <text value="filed"/>
            <date form="text" variable="submitted"/>
          </group>
          <group delimiter=" ">
            <text value="issued"/>
            <date form="text" variable="issued"/>
          </group>
        </group>
      </group>
    </group>
  </macro>
  <macro name="nzl-ibid-with-locator">
    <group delimiter=" ">
      <text term="at" text-case="sentence"/>
      <group delimiter=" ">
        <choose>
          <if locator="paragraph">
            <choose>
              <if locator="paragraph" type="patent">
                <number prefix="[" suffix="]" variable="locator"/>
              </if>
              <else-if jurisdiction="us">
                <label form="short" variable="locator"/>
                <number label-form="short" variable="locator"/>
              </else-if>
              <else>
                <number prefix="[" suffix="]" variable="locator"/>
              </else>
            </choose>
          </if>
          <else-if locator="page">
            <number variable="locator"/>
          </else-if>
          <else>
            <label form="short" variable="locator"/>
            <number label-form="short" variable="locator"/>
          </else>
        </choose>
      </group>
    </group>
  </macro>
  <macro name="jurisdictions-treaty">
    <choose>
      <if position="subsequent">
        <text macro="nzl-treaty-subsequent"/>
      </if>
      <else>
        <text macro="nzl-treaty-first"/>
      </else>
    </choose>
  </macro>
  <macro name="nzl-treaty-first">
    <group delimiter=", ">
      <text variable="title"/>
      <group delimiter=" ">
        <names variable="author">
          <name/>
          <institution and="none" delimiter="&#8211;" institution-parts="short"/>
        </names>
        <number prefix="(" suffix=")" variable="collection-number"/>
        <number variable="volume"/>
        <text variable="container-title"/>
        <number variable="page"/>
        <group delimiter=" " is-parallel="last" prefix="(" suffix=")">
          <group delimiter=", ">
            <group delimiter=" ">
              <text value="adopted"/>
              <date date-parts="year-month-day" form="text" variable="original-date"/>
            </group>
            <group delimiter=" ">
              <text value="opened for signature"/>
              <date date-parts="year-month-day" form="text" variable="available-date"/>
            </group>
            <group delimiter=" ">
              <text value="signed"/>
              <date date-parts="year-month-day" form="text" variable="event-date"/>
            </group>
            <group delimiter=" ">
              <text value="entered into force"/>
              <date date-parts="year-month-day" form="text" variable="issued"/>
            </group>
          </group>
        </group>
      </group>
      <text label-form="short" macro="std-locator-non-page-label"/>
    </group>
  </macro>
  <macro name="nzl-treaty-subsequent">
    <group delimiter=", ">
      <text variable="title"/>
      <choose>
        <if disambiguate="true">
          <group delimiter=" ">
            <names variable="author">
              <name/>
              <institution and="none" delimiter="&#8211;" institution-parts="short"/>
            </names>
            <group delimiter=" " is-parallel="last" prefix="(" suffix=")">
              <group delimiter=", ">
                <group delimiter=" ">
                  <text value="adopted"/>
                  <date date-parts="year-month-day" form="text" variable="original-date"/>
                </group>
                <group delimiter=" ">
                  <text value="opened for signature"/>
                  <date date-parts="year-month-day" form="text" variable="available-date"/>
                </group>
                <group delimiter=" ">
                  <text value="signed"/>
                  <date date-parts="year-month-day" form="text" variable="event-date"/>
                </group>
                <group delimiter=" ">
                  <text value="entered into force"/>
                  <date date-parts="year-month-day" form="text" variable="issued"/>
                </group>
              </group>
            </group>
          </group>
        </if>
      </choose>
      <text label-form="short" macro="std-locator-non-page-label"/>
    </group>
  </macro>
  <macro name="nzl-citation">
    <choose>
      <if type="treaty">
        <text macro="jurisdictions-treaty"/>
      </if>
      <else-if match="all" position="ibid-with-locator" variable="locator">
        <text macro="nzl-ibid-with-locator"/>
      </else-if>
      <else-if match="all" position="ibid" variable="locator">
        <text macro="nzl-ibid-with-locator"/>
      </else-if>
      <else-if position="subsequent">
        <group delimiter=", ">
          <group delimiter=" ">
            <choose>
              <if type="patent">
                <text macro="std-patent-detail"/>
              </if>
              <else>
                <text macro="short-form"/>
                <choose>
                  <if disambiguate="true">
                    <choose>
                      <if match="any" type="book report">
                        <choose>
                          <if variable="hereinafter">
                            <text quotes="true" variable="hereinafter"/>
                          </if>
                        </choose>
                      </if>
                      <else>
                        <choose>
                          <if variable="hereinafter">
                            <text quotes="true" variable="hereinafter"/>
                          </if>
                        </choose>
                      </else>
                    </choose>
                  </if>
                </choose>
              </else>
            </choose>
          </group>
          <group delimiter=", " is-parallel="last">
            <group delimiter=" ">
              <text value="above n"/>
              <text variable="first-reference-note-number"/>
            </group>
            <text macro="pinpoint"/>
          </group>		
        </group>          
      </else-if>
      <else>
        <text macro="nzl-citation-first"/>
      </else>
    </choose>
  </macro>
  <macro name="nzl-citation-first">
    <group delimiter=", ">
      <group delimiter=" ">
        <group delimiter=" ">
          <choose>
            <if match="any" type="patent">
              <choose>
                <if type="patent">
                  <text macro="jurisdictions-patent" strip-periods="true"/>
                </if>
              </choose>
            </if>
            <else-if type="interview personal_communication">
              <choose>
                <if type="interview">
                  <text macro="render-interview"/>
                </if>
                <else-if type="personal_communication">
                  <text macro="render-email"/>
                </else-if>
              </choose>
            </else-if>
            <else>
              <group delimiter=" ">
                <text macro="author-note" strip-periods="true"/>
                <choose>
                  <if match="any" type="article-journal article-magazine">
                    <text macro="render-article-journal"/>
                  </if>
                  <else-if type="chapter">
                    <text macro="render-chapter"/>
                  </else-if>
                  <else-if type="thesis">
                    <text macro="render-thesis"/>
                  </else-if>
                  <else-if type="article-newspaper">
                    <text macro="render-article-newspaper" strip-periods="true"/>
                  </else-if>
                  <else-if match="all" type="paper-conference" variable="container-title">
                    <text macro="render-article-journal"/>
                  </else-if>
                  <else-if type="paper-conference">
                    <text macro="render-paper-conference"/>
                  </else-if>
                  <else-if type="speech">
                    <text macro="render-speech"/>
                  </else-if>
                  <else-if type="book">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if type="report">
                    <text macro="render-report"/>
                  </else-if>
                  <else-if type="song">
                    <text macro="render-podcast"/>
                  </else-if>
                  <else-if match="any" type="webpage post-weblog post">
                    <text macro="render-blogpost"/>
                  </else-if>
                  <else-if type="entry-encyclopedia entry-dictionary">
                    <text macro="render-encyclopedia"/>
                  </else-if>
                  <else-if match="all" type="motion_picture">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if match="all" type="broadcast" variable="container-title">
                    <text macro="render-chapter"/>
                  </else-if>
                  <else-if match="all" type="broadcast">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if match="all" type="video">
                    <text macro="render-article-newspaper"/>
                  </else-if>
                  <else>
                    <group delimiter=" ">
                      <group delimiter=", ">
                        <choose>
                          <if match="none" variable="title">
                            <text font-style="italic" variable="container-title"/>
                          </if>
                          <else>
                            <text font-style="italic" variable="title"/>
                          </else>
                        </choose>
                      </group>
                      <text macro="publisher-place-year"/>
                      <text macro="volume-number"/>
                    </group>
                  </else>
                </choose>
                <text macro="archive-location-and-place"/>
                <text macro="access-url"/>
              </group>
            </else>
          </choose>
          <text macro="hereinafter"/>
        </group>
        <choose>
          <if locator="section">
            <choose>
              <if is-numeric="locator" match="none"/>
              <else>
                <text macro="pinpoint"/>
              </else>
            </choose>
          </if>
          <else>
            <text macro="pinpoint"/>
          </else>
        </choose>
      </group>
      <text macro="pinpoint"/>
    </group>
  </macro>
  <macro name="archive-location-and-place">
    <group delimiter=", ">
      <text variable="archive"/>
      <text variable="archive_location"/>
    </group>
  </macro>
  <macro name="juris-pretitle">
    <text value="[sample pretitle]"/>
  </macro>
  <macro name="juris-title">
    <text variable="title"/>
  </macro>
  <macro name="juris-main">
    <text value="[sample body]"/>
  </macro>
  <macro name="juris-tail">
    <text value="[sample tail]"/>
  </macro>
  <macro name="juris-pretitle-short">
    <text value="[sample pretitle short]"/>
  </macro>
  <macro name="juris-title-short">
    <text value="[sample title short]"/>
  </macro>
  <macro name="juris-main-short">
    <text value="[sample main short]"/>
  </macro>
  <macro name="juris-tail-short">
    <text value="[sample tail short]"/>
  </macro>
  <macro name="juris-locator">
    <text value="[locator]"/>
  </macro>
  <macro name="at-locator-label">
    <text value="at"/>
  </macro>
  <macro name="jurism">
   <choose>
      <if position="ibid-with-locator">
        <group delimiter=" ">
          <text term="ibid" font-style="italic"/>
          <group delimiter=" ">
            <text value="at"/>
            <group require="empty-label">
              <text macro="juris-locator" label-form="symbol"/>
            </group>
          </group>
          <group delimiter=" " reject="empty-label">
            <text macro="juris-locator" label-form="symbol"/>
          </group>
        </group>
      </if>
      <else-if position="ibid">
        <text term="ibid" font-style="italic"/>
      </else-if>
      <else-if>
        <!-- short form -->
        <conditions match="any">
          <condition match="all" position="near-note"/>
          <condition match="all" type="report" variable="title" position="far-note"/>
          <condition match="all" type="report" variable="committee" position="far-note"/>
          <condition match="all" type="hearing" variable="committee" position="far-note"/>
          <condition match="all" type="legal_case" variable="document-name" position="far-note"/>
        </conditions>
        <group delimiter=" ">
          <group delimiter=", ">
            <text macro="juris-pretitle-short"/>
            <choose>
              <if>
                <conditions match="all">
                  <condition type="legal_case"/>
                  <condition match="none" variable="document-name"/>
                </conditions>
                <text macro="juris-title-short" font-style="italic"/>
              </if>
              <else-if type="hearing report">
                <text macro="juris-title-short" font-style="italic"/>
              </else-if>
              <else>
                <text macro="juris-title-short"/>
              </else>
            </choose>
            <choose>
              <if>
                <conditions match="any">
                  <condition match="all" type="hearing" variable="committee"/>
                  <condition match="all" type="report" variable="title"/>
                  <condition match="all" type="report" variable="committee"/>
                  <condition match="all" type="legal_case" variable="document-name"/>
                </conditions>
                <group delimiter=", ">
                  <text macro="juris-main-short"/>
                  <!-- group delimiter=" ">
                    <text value="supra" font-style="italic"/>
                    <text term="note"/>
                    <text variable="first-reference-note-number"/>
                  </group -->
                  <group delimiter=" ">
                    <text value="at"/>
                    <group require="empty-label">
                      <text macro="juris-locator" label-form="symbol"/>
                    </group>
                  </group>
                  <group delimiter=" " reject="empty-label">
                    <text macro="juris-locator" label-form="symbol"/>
                  </group>
                </group>
              </if>
              <else>
                <group delimiter=" ">
                  <group delimiter=", ">
                    <choose>
                      <if>
                        <conditions match="all">
                          <condition type="hearing"/>
                          <condition match="none" variable="committee"/>
                        </conditions>
                        <text macro="juris-main-short" font-variant="small-caps"/>
                      </if>
                      <else-if>
                        <conditions match="all">
                          <condition type="report"/>
                          <condition match="none" variable="committee title"/>
                        </conditions>
                        <text macro="juris-main-short" font-variant="small-caps"/>
                      </else-if>
                      <else>
                        <text macro="juris-main-short"/>
                      </else>
                    </choose>
                  </group>
                </group>
              </else>
            </choose>
          </group>
          <group delimiter=" ">
            <text value="at"/>
            <group require="empty-label">
              <text macro="juris-locator" label-form="symbol"/>
            </group>
          </group>
          <text macro="juris-locator" label-form="symbol"/>
          <text macro="juris-tail-short"/>
        </group>
      </else-if>
      <else>
        <!-- full form -->
        <group delimiter=", ">
          <group delimiter=" ">
            <group delimiter=" ">
              <choose>
                <if type="hearing report">
                  <text macro="juris-pretitle"/>
                </if>
              </choose>
              <choose>
                <if type="hearing report legal_case" match="any">
                  <text macro="juris-title" font-style="italic"/>
                </if>
                <else>
                  <text macro="juris-title"/>
                </else>
              </choose>
              <group delimiter=" ">
                <group delimiter=", ">
                  <choose>
                    <if>
                      <conditions match="all">
                        <condition match="all" type="report"/>
                        <condition match="none" variable="title committee"/>
                      </conditions>
                      <text macro="juris-main"/>
                    </if>
                    <else-if>
                      <conditions match="all">
                        <condition match="all" type="hearing"/>
                        <condition match="none" variable="committee"/>
                      </conditions>
                      <text macro="juris-main"/>
                    </else-if>
                    <else>
                      <text macro="juris-main"/>
                    </else>
                  </choose>
                  <group delimiter=" " require="empty-label">
                    <text value="at"/>
                    <text macro="juris-locator" label-form="symbol"/>
                  </group>
                  <group require="comma-safe">
                    <text macro="juris-locator" label-form="symbol"/>
                  </group>
                </group>
              </group>
            </group>
            <group reject="comma-safe">
              <text macro="juris-locator" label-form="symbol"/>
            </group>
            <group prefix="&#60;" suffix="&#62;">
              <date variable="locator-date">
                <date-part name="year"/>
                <date-part name="month" form="numeric-leading-zeros" prefix="-"/>
                <date-part name="day" form="numeric-leading-zeros" prefix="-"/>
              </date>
            </group>
            <choose>
              <if type="legislation" variable="container-title page volume gazette-flag" match="all"/>
              <else>
                <text macro="juris-tail"/>
              </else>
            </choose>
          </group>
          <choose>
            <if type="legislation" variable="container-title page volume gazette-flag" match="all">
              <text macro="juris-tail"/>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation et-al-min="4" et-al-use-first="1">
    <layout delimiter="; " suffix=".">
      <choose>
        <if>
          <conditions match="all">
            <condition match="any" type="legal_case legislation bill hearing gazette report regulation"/>
            <condition match="all" variable="jurisdiction"/>
          </conditions>
          <text macro="jurism" strip-periods="true"/>
        </if>
        <else>
          <text macro="nzl-citation" strip-periods="true"/>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography et-al-min="21" et-al-use-first="20">
    <sort>
      <key macro="sort-by-type"/>
      <key macro="author-bib"/>
      <key variable="issued"/>
    </sort>
    <layout suffix=".">
      <choose>
        <if>
          <conditions match="all">
            <condition match="any" type="legal_case legislation bill hearing gazette report regulation"/>
            <condition match="all" variable="jurisdiction"/>
          </conditions>
          <text macro="jurism"/>
        </if>
        <else>
          <choose>
            <if type="interview personal_communication">
              <choose>
                <if type="interview">
                  <text macro="render-interview"/>
                </if>
                <else-if type="personal_communication">
                  <text macro="render-email"/>
                </else-if>
              </choose>
            </if>
            <else>
              <group delimiter=" ">
                <text macro="author-bib" strip-periods="true"/>
                <choose>
                  <if type="article-journal article-magazine">
                    <text macro="render-article-journal"/>
                  </if>
                  <else-if type="chapter">
                    <text macro="render-chapter"/>
                  </else-if>
                  <else-if type="thesis">
                    <text macro="render-thesis"/>
                  </else-if>
                  <else-if match="all" type="paper-conference" variable="container-title">
                    <text macro="render-article-journal"/>
                  </else-if>
                  <else-if type="paper-conference">
                    <text macro="render-paper-conference"/>
                  </else-if>
                  <else-if type="article-newspaper">
                    <text macro="render-article-newspaper"/>
                  </else-if>
                  <else-if type="book">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if type="speech">
                    <text macro="render-speech"/>
                  </else-if>
                  <else-if type="report">
                    <text macro="render-report"/>
                  </else-if>
                  <else-if type="song">
                    <text macro="render-podcast"/>
                  </else-if>
                  <else-if match="any" type="webpage post-weblog post">
                    <text macro="render-blogpost"/>
                  </else-if>
                  <else-if type="entry-encyclopedia">
                    <text macro="render-encyclopedia"/>
                  </else-if>
                  <else-if match="all" type="motion_picture" variable="container-title publisher">
                    <text macro="render-chapter"/>
                  </else-if>
                  <else-if match="all" type="motion_picture" variable="publisher">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if match="all" type="motion_picture">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if match="all" type="broadcast" variable="container-title">
                    <text macro="render-chapter"/>
                  </else-if>
                  <else-if match="all" type="broadcast">
                    <text macro="render-book"/>
                  </else-if>
                  <else-if match="all" type="video">
                    <text macro="render-article-newspaper"/>
                  </else-if>
                  <else>
                    <group>
                      <group delimiter=", ">
                        <text font-style="italic" variable="title"/>
                        <text macro="translator"/>
                        <text macro="editor"/>
                      </group>
                      <text macro="publisher-place-year"/>
                      <text macro="volume-number"/>
                    </group>
                  </else>
                </choose>
                <text macro="access-url"/>
              </group>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
