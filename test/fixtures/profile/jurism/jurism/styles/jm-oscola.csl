<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" default-locale="en-GB" page-range-format="minimal-two" version="1.1mlz1" year-range-format="minimal-two">
  <info>
    <title>JM OSCOLA - Oxford Standard for Citation of Legal Authorities</title>
    <id>http://juris-m.github.io/jm-styles/jm-oscola</id>
