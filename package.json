{
  "name": "zotero-better-bibtex",
  "version": "5.0.93",
  "description": "Make Zotero useful for us LaTeX holdouts.",
  "homepage": "https://github.com/retorquere/zotero-better-bibtex/wiki",
  "author": {
    "name": "Emiliano Heyns",
    "email": "Emiliano.Heyns@iris-advies.com"
  },
  "xpi": {
    "name": "Zotero Better Bib(La)Tex",
    "iconURL": "chrome://zotero-better-bibtex/skin/better-bibtex.svg",
    "optionsURL": "chrome://zotero/content/preferences/preferences.xul#better-bibtex",
    "updateLink": "https://github.com/retorquere/zotero-better-bibtex/releases/download/v{version}/zotero-better-bibtex-{version}.xpi",
    "updateInfoURL": "https://retorquere.github.io/zotero-better-bibtex/CHANGELOG.html",
    "releaseURL": "https://github.com/retorquere/zotero-better-bibtex/releases/download/release/"
  },
  "scripts": {
    "test": "cucumber --fail-fast --no-color | tee cucumber.log",
    "ae": "HEADLESS=false KILL=false cucumber --fail-fast --tags @ae",
    "lint": "tslint -t stylish --project .",
    "webpack": "webpack",
    "prebuild": "npm run lint && ts-node setup/index.ts && npm run docs",
    "build": "webpack",
    "postbuild": "zotero-plugin-zipup build zotero-better-bibtex && zotero-plugin-zipup test/fixtures/debug-bridge debug-bridge",
    "minitest": "npm run webpack && node ./minitests/build/dateparser.js",
    "start": "HEADLESS=false KILL=false cucumber --fail-fast --tags @none",
    "s3form": "zotero-plugin-s3form",
    "release": "zotero-plugin-release",
    "branches": "zotero-plugin-branches",
    "prefs": "ts-node zotero-webpack/script/prefs.ts",
    "postversion": "TAG=$(echo $(git describe --tags $(git rev-list --tags --max-count=1))) && git tag -a $TAG $TAG -f -m \"$(git log `git describe --tags --abbrev=0 HEAD^`..HEAD --oneline)\" && git push && git push --tags",
    "running": "ps auxw | grep -i zotero | grep -v grep || true",
    "docs": "ts-node setup/readme.ts && ts-node setup/preferences.ts && typedoc --ignoreCompilerErrors --json typedoc.json content/key-manager && ts-node setup/formatters-doc.ts",
    "ncu": "ncu -a"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "ajv-keywords": "^3.1.0",
    "barcoder": "^2.0.1",
    "better-queue": "^3.8.6",
    "better-queue-memory": "^1.0.2",
    "biblatex-csl-converter": "^1.3.27",
    "citeproc": "^2.1.187",
    "edtf": "^2.3.2",
    "edtfy": "0.0.8",
    "escape-string-regexp": "^1.0.5",
    "eventemitter4": "^0.2.4",
    "fold-to-ascii": "^4.0.0",
    "global": "^4.3.2",
    "he": "^1.1.1",
    "isbnjs": "^0.2.2",
    "issn": "^1.0.5",
    "js-yaml": "^3.10.0",
    "json-stringify-safe": "^5.0.1",
    "json5": "^0.5.1",
    "lokijs": "^1.5.3",
    "node-eta": "^0.9.0",
    "pegjs": "^0.10.0",
    "punycode": "^2.1.0",
    "string-template": "^1.0.0",
    "transliteration": "^1.6.2",
    "xregexp": "^4.1.1",
    "zotero-plugin": "0.0.24"
  },
  "devDependencies": {
    "current-git-branch": "^1.0.1",
    "github": "^13.1.0",
    "@rgrove/parse-xml": "^1.1.1",
    "@types/node": "^9.4.6",
    "@types/webpack": "^3.8.8",
    "archiver": "^2.1.1",
    "aws-s3-form": "^0.3.5",
    "bluebird": "^3.5.1",
    "circular-dependency-plugin": "^4.4.0",
    "clean-webpack-plugin": "^0.1.18",
    "dedent-js": "^1.0.1",
    "dotenv": "^5.0.0",
    "ejs": "^2.5.7",
    "find-up": "^2.1.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "grasp": "^0.6.0",
    "ignore-loader": "^0.1.2",
    "indent-string": "^3.2.0",
    "jsesc": "^2.5.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "node-archiver": "^0.3.0",
    "parallel-webpack": "^2.2.0",
    "pretty-data": "^0.40.0",
    "properties-reader": "^0.0.16",
    "pug": "^2.0.0-rc.4",
    "replace": "^0.3.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.1",
    "sqlite-sync": "^0.3.6",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.2",
    "uri-templates": "^0.2.0",
    "webpack": "^3.11.0",
    "wrapper-webpack-plugin": "^1.0.0",
    "xml-parser": "^1.2.1",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retorquere/zotero-better-bibtex.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/retorquere/zotero-better-bibtex/issues"
  }
}
