{
  "name": "zotero-better-bibtex",
  "version": "5.0.38",
  "description": "Make Zotero useful for us LaTeX holdouts.",
  "homepage": "https://github.com/retorquere/zotero-better-bibtex/wiki",
  "author": {
    "name": "Emiliano Heyns",
    "email": "Emiliano.Heyns@iris-advies.com"
  },
  "xpi": {
    "name": "Zotero Better Bib(La)Tex",
    "iconURL": "chrome://zotero-better-bibtex/skin/better-bibtex.svg",
    "optionsURL": "chrome://zotero/content/preferences/preferences.xul#better-bibtex",
    "updateLink": "https://github.com/retorquere/zotero-better-bibtex/releases/download/v{version}/zotero-better-bibtex-{version}.xpi",
    "updateInfoURL": "https://retorquere.github.io/zotero-better-bibtex/CHANGELOG.html",
    "releaseURL": "https://github.com/retorquere/zotero-better-bibtex/releases/download/release/"
  },
  "scripts": {
    "test": "cucumber --fail-fast --no-color | tee cucumber.log",
    "ae": "HEADLESS=false KILL=false cucumber --fail-fast --tags @ae",
    "lint": "tsc --lib es6 webpack/tslint/noModuleExportsRule.ts && tslint -t stylish --project . --rules-dir ./webpack/tslint",
    "prebuild": "mkdir -p build && rm -rf build/* && mkdir -p xpi && rm -f xpi/*.xpi && mkdir -p gen && rm -rf gen/*",
    "webpack": "npm run prefs && webpack",
    "build": "npm run lint && npm run webpack",
    "postbuild": "node webpack/scripts/zipup.js build zotero-better-bibtex && node webpack/scripts/zipup.js test/fixtures/debug-bridge debug-bridge",
    "minitest": "npm run webpack && node ./minitests/build/dateparser.js",
    "start": "HEADLESS=false KILL=false cucumber --fail-fast --tags @none",
    "s3form": "ts-node webpack/scripts/s3form.ts",
    "release": "ts-node webpack/scripts/release.ts",
    "branches": "ts-node webpack/scripts/branches.ts",
    "prefs": "ts-node webpack/scripts/prefs.ts",
    "postversion": "TAG=$(echo $(git describe --tags $(git rev-list --tags --max-count=1))) && git tag -a $TAG $TAG -f -m \"$(git log `git describe --tags --abbrev=0 HEAD^`..HEAD --oneline)\" && git push && git push --tags",
    "running": "ps auxw | grep -i zotero | grep -v grep || true"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ajv": "^5.3.0",
    "ajv-keywords": "^2.1.1",
    "barcoder": "^2.0.1",
    "better-queue": "^3.8.6",
    "better-queue-memory": "^1.0.2",
    "biblatex-csl-converter": "^1.2.2",
    "edtf": "^2.2.2",
    "edtfy": "0.0.8",
    "escape-string-regexp": "^1.0.5",
    "eventemitter4": "^0.2.4",
    "fold-to-ascii": "^4.0.0",
    "isbnjs": "^0.2.2",
    "issn": "^1.0.5",
    "js-yaml": "^3.10.0",
    "json-stringify-safe": "^5.0.1",
    "json5": "^0.5.1",
    "lokijs": "^1.5.1",
    "node-eta": "^0.9.0",
    "pegjs": "^0.10.0",
    "punycode": "^2.1.0",
    "string-template": "^1.0.0",
    "transliteration": "^1.6.2",
    "xregexp": "^3.2.0"
  },
  "devDependencies": {
    "@rgrove/parse-xml": "^1.1.1",
    "@types/node": "^8.0.53",
    "archiver": "^2.1.0",
    "aws-s3-form": "^0.3.5",
    "bluebird": "^3.5.1",
    "circular-dependency-plugin": "^4.2.1",
    "clean-webpack-plugin": "^0.1.17",
    "dedent-js": "^1.0.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "grasp": "^0.6.0",
    "ignore-loader": "^0.1.2",
    "indent-string": "^3.2.0",
    "jsesc": "^2.5.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "node-archiver": "^0.3.0",
    "npm-check-updates": "^2.13.0",
    "pretty-data": "^0.40.0",
    "pug": "^2.0.0-rc.4",
    "replace": "^0.3.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.7.8",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "uri-templates": "^0.2.0",
    "webpack": "^3.8.1",
    "wrapper-webpack-plugin": "^1.0.0",
    "xml-parser": "^1.2.1",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.24"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retorquere/zotero-better-bibtex.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/retorquere/zotero-better-bibtex/issues"
  }
}
