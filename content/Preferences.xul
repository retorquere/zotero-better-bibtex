                  <textbox rows="10" flex="1" multiline="true" preference="pref-better-bibtex-strings"/>
                  <groupbox>
                    <caption
                      label="&better-bibtex.Preferences.export.fields.export-strings;"
                      bbt:preference="pref-better-bibtex-exportBibTeXStrings"/>
