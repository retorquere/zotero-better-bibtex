                        <radio label="&better-bibtex.Preferences.advanced.import.sentenceCase.on;" value="on"/>
                        <radio label="&better-bibtex.Preferences.advanced.import.sentenceCase.off;" value="off"/>
                      </radiogroup>
                    </groupbox>
